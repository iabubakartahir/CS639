<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">
    
    <!-- Outer Circle -->
    <path
        android:fillColor="@color/primary_blue"
        android:pathData="M100,100m-80,0a80,80 0 1,1 160,0a80,80 0 1,1 -160,0" />
    
    <!-- Inner Circle -->
    <path
        android:fillColor="@color/accent_orange"
        android:pathData="M100,100m-50,0a50,50 0 1,1 100,0a50,50 0 1,1 -100,0" />
    
    <!-- Center Dot -->
    <path
        android:fillColor="@color/white"
        android:pathData="M100,100m-15,0a15,15 0 1,1 30,0a15,15 0 1,1 -30,0" />
    
    <!-- Radius Line -->
    <path
        android:strokeColor="@color/white"
        android:strokeWidth="3"
        android:pathData="M100,100 L180,100" />
    
    <!-- Radius Label -->
    <path
        android:fillColor="@color/white"
        android:pathData="M140,95 L140,105 L150,105 L150,95 Z" />
</vector>

